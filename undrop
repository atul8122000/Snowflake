-- UNDROP command - Tables
DROP TABLE DB_NAME.SCHEMA.TABLE ;
SELECT * FROM DB_NAME.SCHEMA.TABLE ;
UNDROP TABLE DB_NAME.SCHEMA.TABLE ;

-- UNDROP command - Schemas
DROP SCHEMA DB_NAME.SCHEMA;
SELECT * DB_NAME.SCHEMA.TABLE ;
UNDROP SCHEMA DB_NAME.SCHEMA;

-- UNDROP command - Database
DROP DATABASE DB_NAME;
SELECT * FROM DB_NAME.SCHEMA.TABLE ;
UNDROP DATABASE DB_NAME;